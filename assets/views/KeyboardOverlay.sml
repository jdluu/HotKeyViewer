<lane vertical-content-alignment="Middle" horizontal-content-alignment="Middle" orientation="Vertical">
    <panel layout="1000px 500px" background="#AA000000" padding="20">
        <label text="Keyboard Overlay" font="dialogue" horizontal-alignment="Middle" color="#FFFFFF" />
        
        <lane orientation="Vertical" horizontal-content-alignment="Middle" margin="10, 20, 0, 0">
            <!-- Number Row -->
            <lane>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="1" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="2" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="3" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="4" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="5" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
            </lane>
            <!-- QWERTY Row -->
            <lane>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="Q" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="W" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="E" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="R" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="T" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="Y" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
            </lane>
             <!-- ASDF Row -->
             <lane>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="A" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="S" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="D" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="F" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="G" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
                <panel padding="5" background="#333333" margin="2" layout="40px 40px"><label text="H" horizontal-alignment="Middle" vertical-alignment="Middle"/></panel>
            </lane>
        </lane>
    </panel>
</lane>
