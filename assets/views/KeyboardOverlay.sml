<lane vertical-content-alignment="Middle" horizontal-content-alignment="Middle" orientation="Vertical">
    <frame layout="1000px 500px" background-tint="#AA000000" padding="20" background={@Mods/StardewUI/Sprites/MenuBackground}>
        <lane orientation="Vertical">
            <label text="Keyboard Overlay" font="dialogue" horizontal-alignment="Middle" color="#FFFFFF" />
            
            <lane orientation="Vertical" horizontal-content-alignment="Middle" margin="10, 20, 0, 0">
                <!-- Number Row -->
                <lane>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="1" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="2" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="3" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="4" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="5" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                </lane>
                <!-- QWERTY Row -->
                <lane>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="Q" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="W" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="E" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="R" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="T" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="Y" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                </lane>
                 <!-- ASDF Row -->
                 <lane>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="A" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="S" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="D" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="F" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="G" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                    <frame padding="5" background-tint="#333333" margin="2" layout="40px 40px" background={@Mods/StardewUI/Sprites/MenuBackground}><label text="H" horizontal-alignment="Middle" vertical-alignment="Middle"/></frame>
                </lane>
            </lane>
        </lane>
    </frame>
</lane>
